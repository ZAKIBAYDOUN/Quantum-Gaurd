# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/quantum_guard
PGHOST=localhost
PGPORT=5432
PGUSER=username
PGPASSWORD=password
PGDATABASE=quantum_guard

# Application Configuration
NODE_ENV=development
SESSION_SECRET=your-super-secure-session-secret-here

# Replit Configuration (for deployment)
REPLIT_DOMAINS=your-app-name.replit.app
REPL_ID=your-repl-id

# Blockchain Configuration
BLOCKCHAIN_PORT=5470
P2P_PORT=5470
WEBSOCKET_PORT=6470
QNN_API_PORT=5001

# Mining Configuration
MINING_THREADS=4
MINING_DIFFICULTY=4
BLOCK_REWARD=25

# Security Configuration
ENABLE_QNN_VALIDATION=true
ENABLE_ZK_PROOFS=true
ENABLE_ANTI_MEV=true
MPC_THRESHOLD=2

# External APIs (Optional)
# INFURA_PROJECT_ID=your-infura-project-id
# ALCHEMY_API_KEY=your-alchemy-api-key
# COINGECKO_API_KEY=your-coingecko-api-key

# Development Flags
DEBUG_MODE=true
VERBOSE_LOGGING=false
ENABLE_CORS=true

# Network Configuration
MAX_PEERS=100000
PEER_DISCOVERY_TIMEOUT=30000
NETWORK_TIMEOUT=60000

# AI/QNN Configuration
QNN_NEURONS=32
QNN_CONFIDENCE_THRESHOLD=0.75
AI_VALIDATION_TIMEOUT=5000

# Smart Contract Configuration
SOLIDITY_VERSION=0.8.20
HARDHAT_NETWORK=localhost
FOUNDRY_PROFILE=default